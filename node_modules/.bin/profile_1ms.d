#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir//usr/sbin/dtrace" ]; then
  exec "$basedir//usr/sbin/dtrace" -s "$basedir/../perf-sym/profile_1ms.d" "$@"
else 
  exec /usr/sbin/dtrace -s "$basedir/../perf-sym/profile_1ms.d" "$@"
fi
